<?xml version="1.0"?>
<block>
  <name>GI Estimation Decider</name>
  <key>dvbt2rx_gi_est_decider</key>
  <category>[dvbt2rx]</category>
  <import>import dvbt2rx</import>
  <make>dvbt2rx.gi_est_decider()</make>

  <sink>
    <name>in</name>
    <type>float</type>
  </sink>

  <sink>
    <name>in_power</name>
    <type>float</type>
  </sink>

  <sink>
    <name>cfg_in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>


  <source>
    <name>gi_est</name>
    <type>message</type>
    <optional>1</optional>
  </source>

  <source>
    <name>cfg_out</name>
    <type>message</type>
    <optional>1</optional>
  </source>

</block>
